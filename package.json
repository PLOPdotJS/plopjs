    "body-parser": "^1.15.1",
    "express": "^4.13.4",
    "fs-extra": "^0.30.0",
    "jsonwebtoken": "^7.0.0",
    "mongoose": "^4.4.19",
    "nodegit": "^0.13.1",
    "vorpal": "^1.11.1"
  },
  "devDependencies": {
    "chai": "^3.5.0",
